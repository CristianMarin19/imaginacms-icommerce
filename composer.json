{
    "name": "imagina/icommerce-module",
    "description": "Completely customizable eCommerce platform for entrepreneurs worldwide",
    "type": "asgard-module",
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "composer/installers": "~1.0",
        "idavoll/core-module": "~3.0",
        "imagina/ihelpers-module": "~3.0",
        "imagina/notification-module": "~3.1",
        "willvincent/laravel-rateable": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "3.5.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Icommerce\\": ".",
            "Modules\\": "Modules/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Soporte Imagina",
            "email": "soporte@imaginacolombia.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Icommerce\\Providers\\IcommerceServiceProvider"
            ],
            "aliases": {

            }
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Icommerce\\": ""
        }
    }
}
